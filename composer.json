{
    "name": "kanellov/password-strength-validator",
    "type": "library",
    "description": "A validator for checking password strength.cocom",
    "license": "GNU GPLv3",
    "keywords": ["validator","password","zend","zend-validator"],
    "homepage": "https://github.com/kanellov/password-strength-validator",
    "authors": [
        {
            "name": "Vassilis Kanellopoulos",
            "email": "contact@kanellov.com",
            "homepage": "http://kanellov.com"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "1.*",
        "phpunit/PHPUnit": "4.8.0",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpcov": "2.*",
        "zendframework/zend-validator": "^2"
    },
    "suggest": {
        "zendframework/zend-validator": "Needed to support PasswordStrength Zend Validator"
    },
    "autoload": {
        "files": [
            "src/password_strength.php"
        ],
        "psr-4": {
            "Knlv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KnlvTest\\": "test"
        }
    }
}
